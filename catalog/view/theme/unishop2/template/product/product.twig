					'variant': '',
					'price': {{ microdata.price }}
				}]
			}
		}
	});
	
	if (typeof(gtag) === 'function') {
		gtag('event', 'view_item', {
			"items": [{
				'id': {{ product_id }}, 
				'name': "{{ microdata.name }}", 
				'brand': "{{ microdata.manufacturer }}", 
				'category': "{{ microdata.category }}", 
				'variant': '',
